#include <stdio.h>

int main() {
  int sayi,i=2,count=0;
  printf("Bir sayı girin \n");
  scanf("%d",&sayi);
  
  while(i<sayi){
    if(sayi%i == 0){
        count++;
        break;
    }
    i++;
  }
  if(count==0){
    printf("%d bir asal sayı!\n",sayi);
  }
  else{
    printf("%d bir asal sayı değil!\n",sayi);
  }

  return 0;
}
